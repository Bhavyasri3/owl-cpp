public:
    deque<int>q;
    MyStack() {
    }
    
    void push(int x) {
        q.push_back(x);
    }
    
    int pop() {
        int a=q.back();
        q.pop_back();
        return a;
    }
    
    int top() {
        return q.back();
        
    }
    
    bool empty() {
        return q.empty();
    }
};
