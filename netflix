netflix
#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	for (int i=0;i<n;i++)
	{
	    int a,b,c,x;
	    cin>>a>>b>>c>>x;
	    if (a+b>=x ||b+c>=x ||a+c>=x)
	    {
	        cout<<"YES""\n";
	    }
	    else
	    {
	        cout<<"NO""\n";
	    }
	}
	

}
