#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        int a;
        cin>>a;
        int mask=1;
        int c=0;
        while(a>0){
            if(a & mask>0){
                c+=1;

            }
            mask<<=1;
    }
    cout<< c<< endl;
}
}

